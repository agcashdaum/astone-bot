{
  "name": "auction-bot",
  "version": "0.0.2",
  "description": "A bot for chat based auctions",
  "type": "module",
  "main": "server/app.js",
  "scripts": {
    "test": "mocha 'server/test/unit/**/*.spec.js' --require esm",
    "test:e2e": "mocha 'server/test/e2e/**/*.spec.js'",
    "build": "npm run test && npm run clean && npm run babel",
    "deploy": "",
    "start": "node --experimental-modules --es-module-specifier-resolution=node server/app.js",
    "cli": "node --experimental-modules --es-module-specifier-resolution=node server/astone-cli.js",
    "tunnel": "lt --port 9001 --subdomain bidbot",
    "lint": "eslint --cache .",
    "code-check": "plato -r -x test -d report server/"
  },
  "dependencies": {
    "async-polling": "^0.2.1",
    "aws-sdk": "^2.4.8",
    "bl": "^1.1.2",
    "bluebird": "^3.7.1",
    "co-views": "^2.1.0",
    "commander": "^2.9.0",
    "cron": "^1.7.2",
    "dotenv": "^8.2.0",
    "i18n": "^0.8.2",
    "jest": "^24.9.0",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.0.1",
    "koa-compose": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-logger": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^2.0.0",
    "koa-views": "^5.0.2",
    "lodash": "^4.7.0",
    "mongodb": "^3.3.4",
    "msgpack5": "^4.2.1",
    "node-uuid": "^1.4.7",
    "paypal-rest-sdk": "^1.6.8",
    "redis": "^2.6.1",
    "request": "^2.69.0",
    "source-map-support": "^0.4.0",
    "stampit": "^2.1.2",
    "swig": "^1.4.2",
    "text-encoding": "^0.6.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "7.16.0",
    "esm": "^3.2.25",
    "localtunnel": "^1.8.1",
    "mocha": "^6.2.2",
    "mongodb-extended-json": "^1.7.0",
    "nock": "^7.2.2",
    "nodemon": "^1.9.0",
    "path": "^0.12.7",
    "pre-commit": "^1.1.2",
    "redis-mock": "^0.9.0",
    "sinon": "^1.17.3"
  },
  "license": "MIT"
}
